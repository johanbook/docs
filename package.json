{
  "name": "docs",
  "version": "1.1.0",
  "description": "Wiki on computer related topics",
  "main": "index.js",
  "scripts": {
    "format": "prettier --prose-wrap always --write docs/*.md docs/**/*.md",
    "release": "standard-version",
    "prestart": "react-env",
    "start": "node server.js",
    "start:dev": "docusaurus start",
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "prepare": "husky install"
  },
  "author": "Johan Book",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johanbook/docs.git"
  },
  "license": "MIT",
  "homepage": "https://johanbook.com/docs",
  "bugs": {
    "url": "https://github.com/johanbook/docs/issues"
  },
  "lint-staged": {
    "docs/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --prose-wrap always --write"
    ]
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^10.4.0",
    "prettier": "^2.8.2",
    "standard-version": "^9.3.1"
  },
  "dependencies": {
    "@docusaurus/core": "3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@gentledepp/docusaurus-search-local": "1.2.1",
    "@mdx-js/react": "3.1.0",
    "clsx": "2.1.1",
    "file-loader": "^6.2.0",
    "hast-util-is-element": "^1.1.0",
    "prism-react-renderer": "2.4.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rehype-katex": "7.0.1",
    "remark-math": "6.0.0",
    "url-loader": "^4.1.1"
  }
}
