{
  "name": "docs",
  "version": "1.0.0",
  "description": "My comp sci docs",
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages --branch pages -d build",
    "format": "prettier --prose-wrap always --write docs/*.md docs/**/*.md",
    "release": "standard-version",
    "prestart": "react-env",
    "start": "node server.js",
    "start:dev": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "prepare": "husky install"
  },
  "author": "Johan Book",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johanbook/docs.git"
  },
  "license": "MIT",
  "homepage": "https://johanbook.com/docs",
  "bugs": {
    "url": "https://github.com/johanbook/docs/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "docs/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --prose-wrap always --write"
    ]
  },
  "devDependencies": {
    "@docusaurus/core": "2.0.0-beta.9",
    "@docusaurus/preset-classic": "2.0.0-beta.9",
    "@mdx-js/react": "^1.6.21",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.4.0",
    "prettier": "^2.0.5",
    "prism-react-renderer": "^1.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "standard-version": "^9.3.1",
    "url-loader": "^4.1.1"
  },
  "dependencies": {
    "hast-util-is-element": "^1.1.0",
    "rehype-katex": "^4.0.0",
    "remark-math": "^3.0.1"
  }
}
